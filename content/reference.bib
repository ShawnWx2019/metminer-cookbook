	pages = {2233--2244},
	file = {ÂÖ®Êñá:/Users/shawn/Zotero/storage/PWEF3HPN/Dixon Á≠â - 2005 - Stress-induced protein S-glutathionylation in Arabidopsis.pdf:application/pdf},
