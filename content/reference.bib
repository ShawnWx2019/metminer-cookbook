	urldate = {2022-05-14},
	booktitle = {Annual {Plant} {Reviews} online},
	publisher = {John Wiley \& Sons, Ltd},
	author = {Zhang, Xuebin and Gonzalez-Carranza, Zinnia H. and Zhang, Shulin and Miao, Yuchen and Liu, Chang-Jun and Roberts, Jeremy A.},
	year = {2019},
	doi = {10.1002/9781119312994.apr0701},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/9781119312994.apr0701},
	keywords = {/unread, F-box proteins, plant, protein degradation, SCF complex, ubiquitin},
	pages = {307--328},
	file = {Snapshot:/Users/shawn/Zotero/storage/PEIYJR24/9781119312994.html:text/html;Zhang et al. - 2019 - F-Box Proteins in Plants.pdf:/Users/shawn/Zotero/storage/MI33MPVC/Zhang et al. - 2019 - F-Box Proteins in Plants.pdf:application/pdf},
