	keywords = {/unread, Flavonoids, Amino Acid Sequence, Biological Transport, Blueberry Plants, Cloning, Molecular, Fruit, Gene Expression Regulation, Plant, Genes, Plant, Genomics, Humans, Molecular Sequence Data, Phylogeny, Plant Proteins, RNA, Messenger, Sequence Alignment},
