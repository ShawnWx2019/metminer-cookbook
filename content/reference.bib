	file = {ÂÖ®Êñá:/Users/shawn/Zotero/storage/EWT3GIYX/Ou Á≠â - 2023 - Metabolome and transcriptome signatures shed light on the anti-obesity effect of Polygonatum sibiric.pdf:application/pdf},
}

