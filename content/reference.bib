	journal = {Journal of Agricultural and Food Chemistry},
	author = {Wang, Wenzhao and Zhou, Yihui and Wu, Yingling and Dai, Xinlong and Liu, Yajun and Qian, Yumei and Li, Mingzhuo and Jiang, Xiaolan and Wang, Yunsheng and Gao, Liping and Xia, Tao},
