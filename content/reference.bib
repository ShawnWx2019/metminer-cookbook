}

@article{ceballos-laita_effects_2018,
	title = {Effects of manganese toxicity on the protein profile of tomato ({Solanum} lycopersicum) roots as revealed by two complementary proteomic approaches, two-dimensional electrophoresis and shotgun analysis},
