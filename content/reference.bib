@article{iqbal_physiological_2022,
	title = {Physiological {Characteristics} of {Cotton} {Subtending} {Leaf} {Are} {Associated} {With} {Yield} in {Contrasting} {Nitrogen}-{Efficient} {Cotton} {Genotypes}},
