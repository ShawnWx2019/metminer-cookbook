@article{hu_laccase_2020-1,
	title = {The laccase gene {GhLac1} modulates fiber initiation and elongation by coordinating jasmonic acid and flavonoid metabolism},
	volume = {8},
	issn = {2214-5141},
	url = {https://www.sciencedirect.com/science/article/pii/S2214514120300210},
	doi = {10.1016/j.cj.2019.11.006},
	abstract = {Cotton fibers are single cells originating in the epidermis of cotton ovules, and serve as the largest natural fiber source for the textile industry. In theory, all epidermal cells have the potential to develop into fibers, but only 15\%‚Äì25\% of epidermis cells develop into commercially viable lint fibers. We previously showed that GhLac1 participates in cotton defense against biotic stress. Here we report that GhLac1 also has a role in cotton fiber development. GhLac1 RNAi lines in cotton showed increased differentiation of fiber initials from epidermis and shortened fiber length, resulting in unchanged lint percentage. Suppression of GhLac1 expression led to constitutively hyperaccumulated jasmonic acid (JA) and flavonoids in ovules and fiber cells. In vitro ovule culture experiments confirmed the distinct roles of JA and flavonoids in fiber initiation and elongation, and showed that fiber development is spatially regulated by these chemicals: the increased fiber initiation in GhLac1 RNAi lines is caused by hyperaccumulated JA and rutin content during the fiber initiation stage and shortened fiber length is caused by constitutively increased JA and naringenin content during the fiber elongation stage.},
	language = {en},
	number = {4},
	urldate = {2023-03-14},
	journal = {The Crop Journal},
	author = {Hu, Qin and Xiao, Shenghua and Guan, Qianqian and Tu, Lili and Sheng, Feng and Du, Xuezhu and Zhang, Xianlong},
	month = aug,
	year = {2020},
	keywords = {/unread, Cotton, Fiber development, Flavonoids, Jasmonic acid},
	pages = {522--533},
	file = {ScienceDirect Full Text PDF:/Users/shawn/Zotero/storage/YM49XRNY/Hu Á≠â - 2020 - The laccase gene GhLac1 modulates fiber initiation.pdf:application/pdf;ScienceDirect Snapshot:/Users/shawn/Zotero/storage/8AAVBM57/S2214514120300210.html:text/html},
}

