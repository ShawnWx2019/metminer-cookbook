	pages = {37--48},
	file = {Full Text PDF:/Users/shawn/Zotero/storage/CNDMKKIB/Parida Á≠â - 2007 - Alterations in photosynthetic pigments, protein and osmotic components in cotton genotypes subjected.pdf:application/pdf},
}

@article{damelia_antioxidant_2018,
	title = {The antioxidant properties of plant flavonoids: their exploitation by molecular plant breeding},
