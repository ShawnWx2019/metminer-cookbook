	keywords = {/unread},
	pages = {483--500},
	file = {Full Text PDF:/Users/shawn/Zotero/storage/SDPDIXML/Slaten et al. - 2020 - mGWAS Uncovers Gln-Glucosinolate Seed-Specific Int.pdf:application/pdf},
}

@article{chen_metabolite-based_2020-1,
