	year = {2014},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/cem.2567},
	keywords = {/unread, metabolomics, chemometrics, data integration, data structure, dimensionality reduction},
	pages = {1--9},
	file = {Full Text PDF:/Users/shawn/Zotero/storage/MVXSIRZS/Boccard and Rudaz - 2014 - Harnessing the complexity of metabolomic data with.pdf:application/pdf;Snapshot:/Users/shawn/Zotero/storage/WSK57AQN/cem.html:text/html},
