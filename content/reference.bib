	keywords = {/unread, Genes, Plant, Transcriptome, Gene Expression Profiling, Genotype, Zea mays, Crops, Agricultural, Evolution, Molecular, Gene Regulatory Networks, Genetics, Population, Microarray Analysis, Molecular Sequence Annotation, Selection, Genetic},
	pages = {11878--11883},
	file = {Swanson-Wagner et al_2012_Reshaping of the maize transcriptome by domestication.pdf:/Users/shawn/Zotero/storage/9QIU8FEH/Swanson-Wagner et al_2012_Reshaping of the maize transcriptome by domestication.pdf:application/pdf},
