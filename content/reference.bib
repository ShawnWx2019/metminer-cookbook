	keywords = {/unread, Gene Expression Regulation, Plant, Plant Leaves, Plants, Genetically Modified, Arabidopsis, Arabidopsis Proteins, Glutathione Transferase, Indoles, Cytochrome P-450 Enzyme System, Sesquiterpenes, Tobacco, Biosynthetic Pathways, Gene Knockout Techniques, Thiazoles},
	pages = {2697--2710},
