@article{xu_genome-wide_2016,
	title = {Genome-wide identification and characterization of the apple ({Malus} domestica) {HECT} ubiquitin-protein ligase family and expression analysis of their responsiveness to abiotic stresses},
	volume = {291},
	issn = {1617-4623},
	url = {https://doi.org/10.1007/s00438-015-1129-0},
	doi = {10.1007/s00438-015-1129-0},
	abstract = {The ubiquitin-protein ligases (E3s) directly participate in ubiquitin (Ub) transferring to the target proteins in the ubiquitination pathway. The HECT ubiquitin-protein ligase (UPL), one type of E3s, is characterized as containing a conserved HECT domain of approximately 350 amino acids in the C terminus. Some UPLs were found to be involved in trichome development and leaf senescence in Arabidopsis. However, studies on plant UPLs, such as characteristics of the protein structure, predicted functional motifs of the HECT domain, and the regulatory expression of UPLs have all been limited. Here, we present genome-wide identification of the genes encoding UPLs (HECT gene) in apple. The 13 genes (named as MdUPL1‚ÄìMdUPL13) from ten different chromosomes were divided into four groups by phylogenetic analysis. Among these groups, the encoding genes in the intron‚Äìexon structure and the included additional functional domains were quite different. Notably, the F-box domain was first found in MdUPL7 in plant UPLs. The HECT domain in different MdUPL groups also presented different spatial features and three types of conservative motifs were identified. The promoters of each MdUPL member carried multiple stress-response related elements by cis-acting element analysis. Experimental results demonstrated that the expressions of several MdUPLs were quite sensitive to cold-, drought-, and salt-stresses by qRT-PCR assay. The results of this study helped to elucidate the functions of HECT proteins, especially in Rosaceae plants.},
	language = {en},
	number = {2},
	urldate = {2023-07-04},
	journal = {Molecular Genetics and Genomics},
	author = {Xu, Jianing and Xing, Shanshan and Cui, Haoran and Chen, Xuesen and Wang, Xiaoyun},
	month = apr,
