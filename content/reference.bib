@article{hopkins_role_2009,
	title = {Role of {Glucosinolates} in {Insect}-{Plant} {Relationships} and {Multitrophic} {Interactions}},
	volume = {54},
	copyright = {‚≠ê‚≠ê‚≠ê‚≠ê‚≠ê},
	url = {https://doi.org/10.1146/annurev.ento.54.110807.090623},
	doi = {10.1146/annurev.ento.54.110807.090623},
	abstract = {Glucosinolates present classical examples of plant compounds affecting insect-plant interactions. They are found mainly in the family Brassicaceae, which includes several important crops. More than 120 different glucosinolates are known. The enzyme myrosinase, which is stored in specialized plant cells, converts glucosinolates to the toxic isothiocyanates. Insect herbivores may reduce the toxicity of glucosinolates and their products by excretion, detoxification, or behavioral adaptations. Glucosinolates also affect higher trophic levels, via reduced host or prey quality or because specialist herbivores may sequester glucosinolates for their own defense. There is substantial quantitative and qualitative variation between plant genotypes, tissues, and ontogenetic stages, which poses specific challenges to insect herbivores. Even though glucosinolates are constitutive defenses, their levels are influenced by abiotic and biotic factors including insect damage. Plant breeders may use knowledge on glucosinolates to increase insect resistance in Brassica crops. State-of-the-art techniques, such as mutant analysis and metabolomics, are necessary to identify the exact role of glucosinolates.},
	number = {1},
	urldate = {2022-03-30},
	journal = {Annual Review of Entomology},
	author = {Hopkins, Richard J. and van Dam, Nicole M. and van Loon, Joop J.A.},
	year = {2009},
	pmid = {18811249},
	note = {\_eprint: https://doi.org/10.1146/annurev.ento.54.110807.090623},
	keywords = {/unread, Brassicaceae, resistance, allelochemical, constitutive defenses, crucifer, F-box, induced defenses},
	pages = {57--83},
	annote = {Even though intact glucosinolates may confer resistance to insect herbivores (58), the defensive properties of glucosinolates are enhanced upon hydrolysis by the enzyme myrosinase. Myrosinase is a thioglucosidase that is stored in special myrosinase cells found in all plant organs: Â∞ΩÁÆ°ÂÆåÊï¥ÁöÑÁ°´‰ª£Ëë°ËêÑÁ≥ñËã∑ÂèØËÉΩ‰ºöÂØπÊòÜËô´È£üËçâÂä®Áâ©‰∫ßÁîüÊäóÊÄßÔºà58ÔºâÔºå‰ΩÜÂú®Ëä•Â≠êÈÖ∂Ê∞¥Ëß£ÂêéÔºåÁ°´‰ª£Ëë°ËêÑÁ≥ñËã∑ÁöÑÈò≤Âæ°ÁâπÊÄß‰ºöÂ¢ûÂº∫„ÄÇËä•Â≠êÈÖ∂ÊòØ‰∏ÄÁßçÁ°´Ëë°ËêÑÁ≥ñËã∑ÈÖ∂ÔºåÂÇ®Â≠òÂú®ÊâÄÊúâÊ§çÁâ©Âô®ÂÆòÁöÑÁâπÊÆäËä•Â≠êÈÖ∂ÁªÜËÉû‰∏≠Glucosinolates present classical examples of plant compounds affecting insect-plant interactions. They are found mainly in the family Brassicaceae, which includes several important crops. More than 120 different glucosinolates are known. The enzyme myrosinase, which is stored in specialized plant cells, converts glucosinolates to the toxic isothiocyanates. Insect herbivores may reduce the toxicity of glucosinolates and their products by excretion, detoxification, or behavioral adaptations. Glucosinolates also affect higher trophic levels, via reduced host or prey quality or because specialist herbivores may sequester glucosinolates for their own defense. There is substantial quantitative and qualitative variation between plant genotypes, tissues, and ontogenetic stages, which poses specific challenges to insect herbivores. Even though glucosinolates are constitutive defenses, their levels are influenced by abiotic and biotic factors including insect damage. Plant breeders may use knowledge on glucosinolates to increase insect resistance in Brassica crops. State-of-the-art techniques, such as mutant analysis and metabolomics, are necessary to identify the exact role of glucosinolates.: Á°´‰ª£Ëë°ËêÑÁ≥ñËã∑ÊòØÂΩ±ÂìçÊòÜËô´-Ê§çÁâ©Áõ∏‰∫í‰ΩúÁî®ÁöÑÊ§çÁâ©ÂåñÂêàÁâ©ÁöÑÁªèÂÖ∏‰æãÂ≠ê„ÄÇÂÆÉ‰ª¨‰∏ªË¶ÅÂ≠òÂú®‰∫éËä∏ËãîÁßëÔºåÂåÖÊã¨Âá†ÁßçÈáçË¶ÅÁöÑ‰ΩúÁâ©„ÄÇÂ∑≤Áü•Ë∂ÖËøá120Áßç‰∏çÂêåÁöÑÁ°´‰ª£Ëë°ËêÑÁ≥ñËã∑„ÄÇËä•Â≠êÈÖ∂ÂÇ®Â≠òÂú®ÁâπÊÆäÁöÑÊ§çÁâ©ÁªÜËÉû‰∏≠ÔºåÂ∞ÜÁ°´‰ª£Ëë°ËêÑÁ≥ñËã∑ËΩ¨Âåñ‰∏∫ÊúâÊØíÁöÑÂºÇÁ°´Ê∞∞ÈÖ∏ÈÖØ„ÄÇÊòÜËô´È£üËçâÂä®Áâ©ÂèØ‰ª•ÈÄöËøáÊéíÊ≥Ñ„ÄÅËß£ÊØíÊàñË°å‰∏∫ÈÄÇÂ∫îÊù•Èôç‰ΩéÁ°´‰ª£Ëë°ËêÑÁ≥ñËã∑ÂèäÂÖ∂‰∫ßÂìÅÁöÑÊØíÊÄß„ÄÇÁ°´‰ª£Ëë°ËêÑÁ≥ñÈÖ∏Áõê‰πü‰ºöÈÄöËøáÈôç‰ΩéÂÆø‰∏ªÊàñÁåéÁâ©ÁöÑË¥®ÈáèÔºåÊàñËÄÖÂõ†‰∏∫‰∏ì‰∏öÈ£üËçâÂä®Áâ©ÂèØËÉΩ‰∏∫‰∫ÜËá™Ë∫´Èò≤Âæ°ËÄåÈöîÁ¶ªÁ°´‰ª£Ëë°ËêÑÁ≥ñÈÖ∏ÁõêÔºå‰ªéËÄåÂΩ±ÂìçÊõ¥È´òÁöÑËê•ÂÖªÊ∞¥Âπ≥„ÄÇÊ§çÁâ©Âü∫Âõ†Âûã„ÄÅÁªÑÁªáÂíå‰∏™‰ΩìÂèëËÇ≤Èò∂ÊÆµ‰πãÈó¥Â≠òÂú®Â§ßÈáèÁöÑÊï∞ÈáèÂíåË¥®ÈáèÂ∑ÆÂºÇÔºåËøôÂØπÊòÜËô´È£üËçâÂä®Áâ©ÊûÑÊàê‰∫ÜÁâπÊÆäÁöÑÊåëÊàò„ÄÇÂ∞ΩÁÆ°Á°´‰ª£Ëë°ËêÑÁ≥ñËã∑ÊòØÁªÑÊàêÊÄßÈò≤Âæ°Ôºå‰ΩÜÂÖ∂Ê∞¥Âπ≥ÂèóÂåÖÊã¨ÊòÜËô´‰º§ÂÆ≥Âú®ÂÜÖÁöÑÈùûÁîüÁâ©ÂíåÁîüÁâ©Âõ†Á¥†ÁöÑÂΩ±Âìç„ÄÇÊ§çÁâ©ËÇ≤ÁßçÂÆ∂ÂèØ‰ª•Âà©Áî®Á°´‰ª£Ëë°ËêÑÁ≥ñËã∑ÊñπÈù¢ÁöÑÁü•ËØÜÊù•ÊèêÈ´òËä∏ËãîÂ±û‰ΩúÁâ©ÁöÑÊäóËô´ÊÄß„ÄÇÊúÄÂÖàËøõÁöÑÊäÄÊúØÔºåÂ¶ÇÁ™ÅÂèòÂàÜÊûêÂíå‰ª£Ë∞¢ÁªÑÂ≠¶ÔºåÂØπ‰∫éÁ°ÆÂÆöÁ°´‰ª£Ëë°ËêÑÁ≥ñËã∑ÁöÑÁ°ÆÂàá‰ΩúÁî®ÊòØÂøÖË¶ÅÁöÑ„ÄÇ},
