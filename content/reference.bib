	keywords = {/unread, Cotton, Mass spectrometry, Proteomics, Abiotic stress, Pigment biosynthesis},
	pages = {74--84},
	file = {ScienceDirect Snapshot:/Users/shawn/Zotero/storage/2XVRM275/S1874391914001390.html:text/html},
