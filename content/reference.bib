	urldate = {2022-05-14},
	journal = {Plant Physiology},
	author = {Rigal, Adeline and Doyle, Siamsa M. and Ritter, Andr√©s and Raggi, Sara and Vain, Thomas and O‚ÄôBrien, Jos√© Antonio and Goossens, Alain and Pauwels, Laurens and Robert, St√©phanie},
