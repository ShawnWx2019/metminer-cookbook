	pmcid = {PMC3793063},
	keywords = {/unread},
	pages = {857--866},
	file = {PubMed Central Full Text PDF:/Users/shawn/Zotero/storage/A37FGVU7/Tang et al. - 2013 - FAR-RED ELONGATED HYPOCOTYL3 and FAR-RED IMPAIRED .pdf:application/pdf},
