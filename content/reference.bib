	keywords = {/unread, Metabolomics, Data normalization, Batch effect, Independent component analysis, method, Wavelet transform},
	pages = {60--69},
	file = {Full Text:/Users/shawn/Zotero/storage/PZATK8CJ/Deng et al. - 2019 - WaveICA A novel algorithm to remove batch effects.pdf:application/pdf;ScienceDirect Snapshot:/Users/shawn/Zotero/storage/PB9ZRJWF/S0003267019301849.html:text/html},
