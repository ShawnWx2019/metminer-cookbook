	year = {2013},
	keywords = {/unread, F-box protein, Abiotic stress, Maize, Bioinformatics},
	pages = {559--577},
	file = {Full Text PDF:/Users/shawn/Zotero/storage/FJEK2D3K/Jia et al. - 2013 - Genome-wide identification and characterisation of.pdf:application/pdf},
}

