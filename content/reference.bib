	keywords = {/unread, Metabolomics, Gas Chromatography-Mass Spectrometry, Chromatography, Liquid, Tandem Mass Spectrometry, metabolomics, Animals, Bees, decenedioic acid, fatty acids, GC-MS, honey, Honey, UPLC-QTOF-MS},
	pages = {3362--3370},
}

