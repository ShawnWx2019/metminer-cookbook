	keywords = {/unread, Flavonoids, Co-expression, Expression quantitative trait loci, Genome wide association analysis, Local adaptation},
	pages = {113612},
	file = {Full Text:/Users/shawn/Zotero/storage/5XST28QN/Lu et al. - 2021 - Multi-omics analysis provides insights into geneti.pdf:application/pdf;ScienceDirect Snapshot:/Users/shawn/Zotero/storage/SZFLP46E/S0926669021003769.html:text/html},
