@article{tang_far-red_2013,
	title = {{FAR}-{RED} {ELONGATED} {HYPOCOTYL3} and {FAR}-{RED} {IMPAIRED} {RESPONSE1} {Transcription} {Factors} {Integrate} {Light} and {Abscisic} {Acid} {Signaling} in {Arabidopsis1}[{C}][{W}][{OPEN}]},
