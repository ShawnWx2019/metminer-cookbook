	journal = {Industrial Crops and Products},
	author = {Peng, Zhen and Gao, Qiong and Luo, Cheng and Gong, Wenfang and Tang, Shouwu and Zhang, Xiaomeng and Song, Wu and Wang, Zhenzhen and Liu, Hailiang and Du, Xiongming and Liu, Haifeng},
