	pmid = {26116072},
	pmcid = {PMC4482290},
	keywords = {/unread, ExpPal},
	file = {PubMed Central Full Text PDF:/Users/shawn/Zotero/storage/ZANRZRPD/Tuttle Á≠â„ÄÇ - 2015 - Metabolomic and transcriptomic insights into how c.pdf:application/pdf},
}

