	urldate = {2023-07-26},
	journal = {Plant Physiology},
	author = {Tang, Weijiang and Ji, Qiang and Huang, Yongping and Jiang, Zhimin and Bao, Manzhu and Wang, Haiyang and Lin, Rongcheng},
	month = oct,
	year = {2013},
