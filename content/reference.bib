	journal = {Plant Physiology},
	author = {Slaten, Marianne L. and Yobi, Abou and Bagaza, Clement and Chan, Yen On and Shrestha, Vivek and Holden, Samuel and Katz, Ella and Kanstrup, Christa and Lipka, Alexander E. and Kliebenstein, Daniel J. and Nour-Eldin, Hussam Hassan and Angelovici, Ruthie},
	month = jun,
