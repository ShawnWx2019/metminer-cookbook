	keywords = {/unread, Humans, Metabolomics, Biomarkers, Animals, Aortic Aneurysm, Aortic aneurysm and dissection, Dissection, Macrophage, Mice, Oxoglutarate dehydrogenase, Succinate, Succinic Acid},
	pages = {4373--4385},
}

