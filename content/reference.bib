@incollection{maruta_ascorbate_2018,
	address = {Cham},
	title = {Ascorbate {Peroxidase} {Functions} in {Higher} {Plants}: {The} {Control} of the {Balance} {Between} {Oxidative} {Damage} and {Signaling}},
