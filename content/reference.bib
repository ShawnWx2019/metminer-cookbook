	keywords = {/unread, Gene Expression Regulation, Plant, Phylogeny, Genome, Plant, Gene Expression Profiling, Evolution, Molecular, Gossypium, Crosses, Genetic, Hybridization, Genetic, Polyploidy, Sequence Homology, Nucleic Acid, Genes, Duplicate, Bias, Genetic Speciation},
	pages = {184--193},
