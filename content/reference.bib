	pmcid = {PMC1851844},
	keywords = {/unread, Gene Expression Regulation, Plant, Phylogeny, Genome, Plant, Chromosome Mapping, Oryza, Light, Seeds, Polymerase Chain Reaction, F-Box Proteins},
	pages = {1467--1483},
	file = {Full Text:/Users/shawn/Zotero/storage/FIXXHMQM/Jain et al. - 2007 - F-box proteins in rice. Genome-wide analysis, clas.pdf:application/pdf},
