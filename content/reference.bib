@article{graham_flavonoid_1998,
	title = {Flavonoid and flavonol glycoside metabolism in {Arabidopsis}},
	volume = {36},
	issn = {0981-9428},
	url = {https://www.sciencedirect.com/science/article/pii/S0981942898800983},
