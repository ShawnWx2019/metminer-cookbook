	keywords = {/unread, Flavonoids, Biological Transport, Arabidopsis, Indoleacetic Acids, Plant Roots, Carrier Proteins, Kaempferols, Quercetin, Aminopeptidases, Herbicides, Iodoacetates, Microsomes, Phthalimides},
	pages = {315--324},
}

