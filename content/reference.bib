	author = {Seifert, Felix and Thiemann, Alexander and Grant-Downton, Robert and Edelmann, Susanne and Rybka, Dominika and Schrag, Tobias A. and Frisch, Matthias and Dickinson, Hugh G. and Melchinger, Albrecht E. and Scholten, Stefan},
	year = {2018},
	pmid = {29441076},
	pmcid = {PMC5797689},
	note = {SC: None[s0] },
	keywords = {/unread},
	pages = {13},
	file = {ÂÖ®Êñá:/Users/shawn/Zotero/storage/I26K6XT8/Seifert Á≠â„ÄÇ - 2018 - Parental Expression Variation of Small RNAs Is Neg.pdf:application/pdf},
}

