	pages = {e86344},
	file = {Full Text PDF:/Users/shawn/Zotero/storage/HNSWN7H3/Xiao Á≠â - 2014 - Transcriptome and Biochemical Analyses Revealed a Detailed Proanthocyanidin Biosynthesis Pathway in .pdf:application/pdf},
