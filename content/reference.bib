	keywords = {/unread, Mass Spectrometry, Chromatography, High Pressure Liquid, Animals, Biotransformation, Pharmaceutical Preparations, Anti-HIV Agents, Drug Residues, Hepatocytes, In Vitro Techniques, Indicators and Reagents, Rats, Ritonavir},
