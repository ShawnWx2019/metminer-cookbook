	keywords = {/unread, Brassicaceae, Flovonoids, Naringenin, Polar auxin transport, Root growth, Scutellarein, Scutellarin, WUS-RELATED HOMEOBOX 5},
	pages = {112702},
	file = {ScienceDirect Full Text PDF:/Users/shawn/Zotero/storage/XHZQU2XV/Zhang Á≠â - 2021 - Polar auxin transport May Be responsive to specific features of flavonoid structure.pdf:application/pdf;ScienceDirect Snapshot:/Users/shawn/Zotero/storage/WMZTXCMA/S0031942221000492.html:text/html},
