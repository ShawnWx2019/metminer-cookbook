	keywords = {/unread, Metabolomics, CE-MS, Data, False-discovery rate, Imputation, k-nearest neighbour, Missing values},
	pages = {3050--3060},
	file = {Full Text PDF:/Users/shawn/Zotero/storage/8W6SIPLY/Armitage et al. - 2015 - Missing value imputation strategies for metabolomi.pdf:application/pdf;Snapshot:/Users/shawn/Zotero/storage/9Y2A2NP4/elps.html:text/html},
