	author = {Xu, Chaohua and Mao, Jun and Li, Xujuan and Burner, David M. and Li, Chunjia and Hussin Hussin, Shareif H. and Lin, Xiuqin and Liu, Hongbo and Zhao, Peifang and Lu, Xin and Liu, Xinlong},
