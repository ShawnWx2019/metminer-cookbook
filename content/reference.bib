	journal = {Plant Physiology},
	author = {Zhou, Xiao Feng and Jin, Yin Hua and Yoo, Chan Yul and Lin, Xiao-Li and Kim, Woe-Yeon and Yun, Dae-Jin and Bressan, Ray A. and Hasegawa, Paul M. and Jin, Jing Bo},
