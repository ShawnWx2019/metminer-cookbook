	pmid = {25315323},
	note = {SC: 0000045 
Publisher: American Society of Plant Biologists
Section: Research Article},
	keywords = {/unread},
	pages = {3939--3948},
	file = {Full Text PDF:/Users/shawn/Zotero/storage/CTGYPBJT/Paschold Á≠â„ÄÇ - 2014 - Nonsyntenic Genes Drive Highly Dynamic Complementa.pdf:application/pdf;Snapshot:/Users/shawn/Zotero/storage/SR5GDS9S/3939.html:text/html},
