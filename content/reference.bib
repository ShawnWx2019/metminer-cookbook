	journal = {Frontiers in Plant Science},
	author = {Warinowski, Tino and Koutaniemi, Sanna and K√§rk√∂nen, Anna and Sundberg, Ilari and Toikka, Merja and Simola, Liisa Kaarina and Kilpel√§inen, Ilkka and Teeri, Teemu H.},
	year = {2016},
	note = {üè∑Ô∏è /unread},
	keywords = {/unread},
	file = {Full Text PDF:/Users/shawn/Zotero/storage/MXGASQ32/Warinowski Á≠â - 2016 - Peroxidases Bound to the Growing Lignin Polymer Produce Natural Like Extracellular Lignin in a Cell.pdf:application/pdf},
}

