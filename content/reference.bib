	author = {Owens, Daniel K. and Crosby, Kevin C. and Runac, Justin and Howard, Brad A. and Winkel, Brenda S. J.},
	month = oct,
	year = {2008},
	keywords = {/unread, Flavonoids, Alleles, 3-Deoxy anthocyanidins, Enzyme characteristics, Genetic analysis, Seed coat color},
	pages = {833--843},
}

