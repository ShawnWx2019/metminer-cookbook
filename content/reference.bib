	author = {Liu, Wen-Yu and Yu, Chun-Ping and Chang, Chao-Kang and Chen, Hsiang-June and Li, Meng-Yun and Chen, Yi-Hua and Shiu, Shin-Han and Ku, Maurice S. B. and Tu, Shih-Long and Lu, Mei-Yeh Jade and Li, Wen-Hsiung},
